                                        }
                                    },
                                    { "key_code": "right_shift" }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "right_shift pressed",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
