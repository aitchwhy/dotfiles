{
  "name": "@dotfiles/factory",
  "version": "1.0.0",
  "type": "module",
  "description": "Universal Project Factory - Generate formally consistent software systems",
  "scripts": {
    "dev": "bun run --watch src/cli.ts",
    "start": "bun run src/cli.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "bunx biome check .",
    "lint:fix": "bunx biome check --write .",
    "lint:ast": "sg scan",
    "test:rules": "sg test",
    "format": "bunx biome format --write .",
    "validate": "bun run typecheck && bun run lint && bun run lint:ast && bun test"
  },
  "dependencies": {
    "effect": "3.19.9",
    "@effect/cli": "0.72.1",
    "@effect/platform": "0.93.6",
    "@effect/platform-node": "0.103.0",
    "@effect/platform-bun": "0.86.0",
    "@effect/printer": "0.47.0",
    "@effect/printer-ansi": "0.47.0",
    "handlebars": "4.7.8",
    "oxc-parser": "0.101.0",
    "oxc-transform": "0.101.0",
    "@ast-grep/napi": "0.33.1",
    "yaml": "2.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bun": "1.2.10",
    "typescript": "5.9.3"
  },
  "engines": {
    "bun": ">=1.3.4"
  }
}
