{{ if eq .chezmoi.os "darwin" -}}
#!/bin/bash

brew bundle install --no-lock --file=/dev/stdin <<EOF
{{ range .packages.darwin.brews -}}
brew {{ . | quote }}
{{ end -}}

{{ range .packages.darwin.casks -}}
cask {{ . | quote }}
{{ end -}}

{{ range .packages.darwin.mas -}}
mas {{ .name | quote }}, id: {{ .id }}
{{ end -}}

{{ range .packages.darwin.vscode -}}
vscode {{ . | quote }}
{{ end -}}

EOF
{{ end -}}
